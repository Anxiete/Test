<%= render "card/header" %>
<div class="container-fluid">
    <div class="row">
        <div class="column-margin col-lg-9 mx-auto p-2">
            <h1 class="fw-bold">Contact</h1>
            <nav aria-label="breadcrumb ">
                <ol class="breadcrumb pt-3">
                    <li class="breadcrumb-item"><a href="<%= homepage_path %>">Home</a></li>
                    <li class="breadcrumb-item active" aria-current="page"><a href="">Contact</a></li>
                </ol>
            </nav>
            <hr class="mt-3">
        </div>  
    </div>
    <div class="col-lg-9 mt-3 mx-auto p-2">
        <% @contacts.each do | contact | %>
        <p><%= contact.content %></p>
        <% end %>
    </div>
</div>
